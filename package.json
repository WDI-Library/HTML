{
  "name": "html-and-css",
  "version": "1.0.0",
  "description": "An introduction to HTML & CSS for use on the GA Web Development Immersive course.",
  "main": "index.js",
  "scripts": {
    "docs:prepare": "gitbook install",
    "docs:watch": "npm run docs:prepare && gitbook serve",
    "docs:build": "npm run docs:prepare && rm -rf _book && gitbook build",
    "docs:publish": "npm run docs:build && gh-pages -d _book"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WDI-Library/html-and-css.git"
  },
  "author": "Alex Chin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/WDI-Library/html-and-css/issues"
  },
  "homepage": "https://github.com/WDI-Library/html-and-css#readme",
  "devDependencies": {
    "gitbook-cli": "^2.3.0"
  },
  "dependencies": {
    "gh-pages": "^0.11.0"
  }
}
